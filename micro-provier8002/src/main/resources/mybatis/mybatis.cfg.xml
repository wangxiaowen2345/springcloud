<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <!-- 环境, 就是配置数据库访问环境的标签.
    default - 默认使用什么环境
    -->
  <settings>
    <setting name="cacheEnabled" value="true"></setting><!--开启二级缓存-->
  </settings>
</configuration>